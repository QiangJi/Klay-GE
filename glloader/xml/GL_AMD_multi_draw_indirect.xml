		<function name="glMultiDrawArraysIndirectAMD" return="void">
			<params>
				<param type="GLenum" name="mode"/>
				<param type="const GLvoid*" name="indirect"/>
				<param type="GLsizei" name="primcount"/>
				<param type="GLsizei" name="stride"/>
			</params>
		</function>
