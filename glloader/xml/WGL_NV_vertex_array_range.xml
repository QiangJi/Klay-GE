<?xml version='1.0'?>

<gl_extension name="WGL_NV_vertex_array_range" reg_no="190">
	<functions>
		<function name="wglAllocateMemoryNV" return="void*">
			<params>
				<param type="GLsizei" name="size"/>
				<param type="GLfloat" name="readfreq"/>
				<param type="GLfloat" name="writefreq"/>
				<param type="GLfloat" name="priority"/>
			</params>
		</function>
		<function name="wglFreeMemoryNV" return="void">
			<params>
				<param type="void*" name="pointer"/>
			</params>
		</function>
	</functions>
</gl_extension>
